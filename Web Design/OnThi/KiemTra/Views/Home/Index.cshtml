@{
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<HangHoa>

<partial name="NguyTheQuang_Body" model="Model" />

@section Scripts {
    <script>
        $("ul li a").on('click', function () {
            var id = $(this).attr("id")
            alert(id);
            $.ajax({
                url: "/Home/HangHoaTheoLoai",
                data: { maloaihang: id },
                success: function (res) {
                    $("#contentLocTheoHangHoa").html(res)
                }
            })
        })
    </script>
}
